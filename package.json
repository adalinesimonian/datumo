{
  "name": "datumo",
  "version": "0.0.0",
  "description": "Lightweight data-modelling",
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/server.js",
    "compile": "gulp babel",
    "compile-watch": "gulp babel-watch",
    "prepublish": "npm run compile",
    "test": "eslint src/**/*.es6 && echo Code looks good!"
  },
  "files": [
    "lib"
  ],
  "babel": {
    "blacklist": [
      "es6.classes",
      "es6.forOf",
      "regenerator",
      "es6.arrowFunctions",
      "es6.constants",
      "es6.blockScoping"
    ]
  },
  "engines": {
    "node": ">=4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vsimonian/datumo.git"
  },
  "author": "Vartan Simonian",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "del": "^2.0.2",
    "eslint": "^1.4.1",
    "eslint-config-standard": "^4.3.2",
    "eslint-plugin-standard": "^1.3.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^5.2.1",
    "gulp-plumber": "^1.0.1",
    "gulp-watch": "^4.3.5",
    "through2": "^2.0.0"
  },
  "dependencies": {
    "lx-valid": "^0.5.6"
  }
}
